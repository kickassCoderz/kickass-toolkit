{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**"]
        },
        "typecheck": {
            "dependsOn": []
        },
        "test": {
            "dependsOn": [],
            "outputs": ["coverage/**"]
        },
        "test:watch": {
            "cache": false
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "clean": {
            "cache": false
        }
    }
}
