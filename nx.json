{
    "npmScope": "kickass-toolkit",
    "affected": {
        "defaultBase": "main"
    },
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "nx/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "generators": {
        "@nrwl/react": {
            "application": {
                "babel": true
            }
        },
        "@nrwl/next": {
            "application": {
                "style": "css",
                "linter": "eslint"
            }
        }
    }
}
