{
    "name": "@kickass-coderz/react-widgets",
    "description": "Production grade React widgets. Unstyled, typed, tested and documented.",
    "version": "0.0.0",
    "license": "MIT",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "scripts": {
        "test": "jest --coverage --passWithNoTests",
        "test:watch": "jest --watch",
        "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf coverage",
        "typecheck": "tsc --noEmit --pretty"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0"
    },
    "dependencies": {
        "@kickass-coderz/hooks": "workspace:*"
    },
    "devDependencies": {
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "eslint-config-custom": "workspace:*",
        "tsconfig": "workspace:*",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "volta": {
        "extends": "../../package.json"
    }
}
