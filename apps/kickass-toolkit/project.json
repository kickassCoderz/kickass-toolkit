{
    "projectType": "application",
    "sourceRoot": "apps/kickass-toolkit/src",
    "targets": {
        "build": {
            "executor": "@nx-plus/docusaurus:browser",
            "options": {
                "outputPath": "dist/apps/kickass-toolkit"
            }
        },
        "serve": {
            "executor": "@nx-plus/docusaurus:dev-server",
            "options": {
                "port": 3000
            }
        },
        "tsc": {
            "executor": "nx:run-commands",
            "options": {
                "commands": [
                    {
                        "command": "cd apps/kickass-toolkit && tsc -b"
                    }
                ]
            }
        }
    },
    "tags": []
}
